{
  "dependencies": {
    "jquery": "2.1.1",
    "angular": "1.3.0",
	"angular-sanitize": "1.3.0",
	"underscore": "1.7.0",
	"underscore.string": "2.3.2"
  },
  "deploy": {
    "jquery": {
      "files": [
        {
          "src": "jquery-2.1.1/dist/jquery.js",
          "dest": [
            "js/lib/jquery.js"
          ]
        }
      ]
    },
    "angular": {
      "files": [
        {
          "src": "angular-1.3.0/angular.js",
          "dest": [
            "js/lib/angular.js"
          ]
        }
      ]
    },
	"angular-sanitize": {
      "files": [
        {
          "src": "angular-sanitize-1.3.0/angular-sanitize.js",
          "dest": [
            "js/lib/angular/angular.sanitize.js"
          ]
        }
      ]
    },
	"underscore": {
      "files": [
        {
          "src": "underscore-1.7.0/underscore.js",
          "dest": [
            "js/lib/underscore.js"
          ]
        }
      ]
    },
	"underscore.string": {
      "files": [
        {
          "src": "underscore.string-2.3.2/lib/underscore.string.js",
          "dest": [
            "js/lib/underscore.string.js"
          ]
        }
      ]
    }
  }
}
