{
  "name": "jsnbt-strap-angular",
  "version": "1.0.1",
  "homepage": "",
  "authors": [
    
  ],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "jquery": "~2.1.1",
    "angular": "~1.3.0",
	"angular-sanitize": "~1.3.0",
	"underscore": "~1.7.0"
  },
  "deploy": {
    "jquery": {
      "files": [
        {
          "src": "jquery/dist/jquery.js",
          "dest": [
            "js/lib/a-jquery.js"
          ]
        }
      ]
    },
    "angular": {
      "files": [
        {
          "src": "angular/angular.js",
          "dest": [
            "js/lib/b-angular.js"
          ]
        }
      ]
    },
	"angular-sanitize": {
      "files": [
        {
          "src": "angular-sanitize/angular-sanitize.js",
          "dest": [
            "js/lib/angular-lib/angular.sanitize.js"
          ]
        }
      ]
    },
	"underscore": {
      "files": [
        {
          "src": "underscore/underscore.js",
          "dest": [
            "js/lib/underscore/underscore.js"
          ]
        }
      ]
    }
  }
}
